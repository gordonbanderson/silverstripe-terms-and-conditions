{
  "name": "suilven/silverstripe-terms-and-conditions",
  "type": "silverstripe-vendormodule",
  "description": "Add a terms and conditions field to a SilverStripe form.",
  "homepage": "https://github.com/suilven/silverstripe-terms-and-conditions",
  "keywords": ["terms","and","conditions","terms","conditions","legal"],
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Gordon Anderson",
      "role": "Developer",
      "email": "gordon.b.anderson@gmail.com",
      "homepage": "https://github.com/gordonbanderson"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "silverstripe/framework": "^4@dev"
  },
  "autoload": {
    "psr-4": {
      "Suilven\\TermsAndConditions\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    },
    "expose": [
      "client/dist",
      "javascript"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
